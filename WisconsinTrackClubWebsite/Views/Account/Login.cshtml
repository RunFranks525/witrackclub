@using WisconsinTrackClubWebsite.Models
@model LoginViewModel

@{
    ViewBag.Title = "Log in";
}

<div class="container">
    <div class="row">
        <div class="col-md-12 information">
            <h2>@ViewBag.Title</h2>
            <h4>Register or Login through your Facebook account</h4>
            <section id="socialLoginForm">
                @Html.Partial("_ExternalLoginsListPartial", new ExternalLoginListViewModel { Action = "ExternalLogin", ReturnUrl = ViewBag.ReturnUrl })
            </section>
        </div>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}