@model IEnumerable<WisconsinTrackClubWebsite.Models.UserRunEntry>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12 information">
        <h2>Index</h2>

        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.UserRunEntryId }) |
                        @Html.ActionLink("Details", "Details", new { id = item.UserRunEntryId }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.UserRunEntryId })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>

